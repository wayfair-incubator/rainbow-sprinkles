{
  "name": "rainbow-sprinkles",
  "version": "0.10.2",
  "description": "Responsive, theme-based style props for Vanilla Extract",
  "main": "dist/rainbow-sprinkles.cjs.js",
  "module": "dist/rainbow-sprinkles.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wayfair-incubator/rainbow-sprinkles.git"
  },
  "keywords": [
    "vanilla-extract",
    "styled-system",
    "css",
    "system-props",
    "react",
    "design-tokens",
    "design-system"
  ],
  "author": "Rogin Farrer <rogin@roginfarrer.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wayfair-incubator/rainbow-sprinkles/issues"
  },
  "homepage": "https://github.com/wayfair-incubator/rainbow-sprinkles#readme",
  "files": [
    "/dist",
    "/createRuntimeRainbowSprinkles"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/rainbow-sprinkles.esm.js",
      "default": "./dist/rainbow-sprinkles.cjs.js"
    },
    "./createRuntimeRainbowSprinkles": {
      "module": "./createRuntimeRainbowSprinkles/dist/rainbow-sprinkles-createRuntimeRainbowSprinkles.esm.js",
      "default": "./createRuntimeRainbowSprinkles/dist/rainbow-sprinkles-createRuntimeRainbowSprinkles.cjs.js"
    }
  },
  "scripts": {
    "test": "pnpm -w test"
  },
  "devDependencies": {
    "@vanilla-extract/css": "^1.7",
    "@vanilla-extract/dynamic": "^2",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1",
    "@vanilla-extract/dynamic": "^2"
  },
  "dependencies": {
    "lodash.merge": "^4.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "createRuntimeRainbowSprinkles.ts"
    ]
  }
}
